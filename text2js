#!/bin/bash
echo "/* Mode: Javascript; Character-encoding: utf-8; */" > $3
echo >> $3
echo "/* DO NOT EDIT THIS FILE!! It is automatically */" >> $3
echo "/*   generated from the file \"$2\" */" >> $3
echo >> $3
echo "$1=" >> $3;
sed s/\\\"/\\\\\"/g $2 | sed "s/.*/    \"&\\\n\"+/g" >> $3;
echo -n $'    "";' >> $3;
echo >> $3;
